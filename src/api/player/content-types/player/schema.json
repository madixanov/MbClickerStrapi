{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "player"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "telegram_id": {
      "type": "integer",
      "unique": true
    },
    "username": {
      "type": "string"
    },
    "photo_url": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "clicks": {
      "type": "biginteger",
      "default": "0"
    },
    "level": {
      "type": "integer",
      "default": 1
    },
    "progress_tokens": {
      "type": "biginteger"
    },
    "bonus_given": {
      "type": "boolean",
      "default": false
    },
    "invite_code": {
      "type": "string",
      "unique": true
    },
    "invited_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::player.player",
      "inversedBy": "invited_friends"
    },
    "invited_friends": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::player.player",
      "mappedBy": "invited_by"
    },
    "completed_bonuses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::bonus.bonus",
      "mappedBy": "completedBy"
    },
    "completed_tasks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::task.task",
      "inversedBy": "completedBy"
    }
  }
}
